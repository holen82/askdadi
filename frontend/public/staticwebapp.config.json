{
  "routes": [
    {
      "route": "/login",
      "rewrite": "/.auth/login/google?post_login_redirect_uri=/"
    }
  ],
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": [
      "/api/*",
      "/.auth/login/*",
      "/.auth/logout",
      "/assets/*"
    ]
  },
  "responseOverrides": {
    "401": {
      "redirect": "/.auth/login/google?post_login_redirect_uri=/",
      "statusCode": 302
    }
  }
}