{
  "routes": [
    {
      "route": "/login",
      "rewrite": "/.auth/login/google"
    }
  ],
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": ["/api/*", "/.auth/*", "/assets/*"]
  },
  "responseOverrides": {
    "401": {
      "redirect": "/.auth/login/google",
      "statusCode": 302
    }
  },
  "auth": {
    "identityProviders": {
      "customOpenIdConnectProviders": {}
    }
  }
}
