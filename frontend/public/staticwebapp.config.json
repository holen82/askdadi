{
  "routes": [
    {
      "route": "/login",
      "rewrite": "/.auth/login/google?post_login_redirect_uri=/"
    }
  ],
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": [
      "/api/*",
      "/.auth/login/*",
      "/.auth/logout",
      "/assets/*"
    ]
  },
  "responseOverrides": {
    "401": {
      "redirect": "/.auth/login/google?post_login_redirect_uri=/",
      "statusCode": 302
    }
  },
  "auth": {
    "identityProviders": {
      "customOpenIdConnectProviders": {
        "google": {
          "registration": {
            "clientIdSettingName": "GOOGLE_CLIENT_ID",
            "clientSecretSettingName": "GOOGLE_CLIENT_SECRET_APP_SETTING_NAME",
            "openIdConnectConfiguration": {
              "wellKnownOpenIdConfiguration": "https://accounts.google.com/.well-known/openid-configuration"
            }
          },
          "login": {
            "nameClaimType": "http://schemas.xmlsoap.org/ws/2005/05/identity/claims/name",
            "scopes": ["openid", "profile", "email"]
          }
        }
      }
    }
  }
}